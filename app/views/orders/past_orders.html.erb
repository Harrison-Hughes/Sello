<h1>Orders</h1>
<%@orders.each do |order|%>
    <%=link_to "order details", order_path(order.id)%><br>
    Order address: <%=order.address%><br>
    Order total: £<%="%.2f" % (order.total_cost)%><br>
    With shipping: £<%="%.2f" % (order.total_cost + 5)%><br>
    Date: <%=order.created_at.to_date%><br><br>
<%end%>
